  /* Markdown */
  .markdown * {
    font-size: 18px;
  }

  .markdown h2 {
    font-size: 30px;
    font-weight: bold;
    padding: 10px 0;
  }

  .markdown ul {
    list-style-type: disc;
    margin: 10px 0;
    padding-left: 20px;
  }

  .markdown ol {
    list-style-type: disc;
    margin: 10px 0;
    padding-left: 20px;
  }

  .markdown ul > li + li {
    margin-top: 5px;
  }

  .markdown img {
    margin-left: auto;
    margin-right: auto;
    margin-top: 25px;
    margin-bottom: 5px;
  }
</style>
